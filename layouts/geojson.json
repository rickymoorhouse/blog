{
 "type":"FeatureCollection",
 "features": [{{ range $i, $e := $.Site.Data.travel }}{{ if .lat }}
 { 
  "type":"Feature", 
  "geometry": { 
   "type":"Point", 
   "coordinates": [{{ .lng }},{{ .lat }}]
  },
  "properties": {
   "title":"{{ .title }}",
   {{ if .photo }}"image":"{{ .photo }}",{{ end }}
   {{ if .link }}"url":"{{ .link }}",{{ end }}
   {{ $date := or .date .datePosted }}{{ $year := substr $date 0 4 }}{{ if and $date (not (eq $year "None")) (not (eq $year "Uni")) }}"year":"{{ $year }}",{{ end }}
   "marker-symbol":"circle",
   "marker-size":"small",
   "marker-color":"{{ $.Site.Params.tertiary | default "#869ba0" }}"
  }
 },
{{ end }}{{ end }}
 {}
 ]
}
